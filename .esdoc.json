{
  "source": "./src",
  "destination": "./docs",
  "excludes": [
    "index.js$",
    "__mocks__",
    ".test.js$",
    "registerServiceWorker.js$",
    "setupTests.js$"
  ],
  "plugins": [
    {"name": "esdoc-standard-plugin"},
    {"name": "esdoc-jsx-plugin"},
    {"name": "esdoc-ecmascript-proposal-plugin", "option": {"all": true}}
  ]
}
